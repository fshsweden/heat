<%= simple_form_for(@property) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :property_id %>
    <%= f.input :address %>
    <%= f.input :picture_take_at %>
    <%= f.association :user %>
    <%= f.input :image %>
    <%= f.input :image_filename %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
